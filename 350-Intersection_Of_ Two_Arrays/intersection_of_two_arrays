class Solution:
    def intersect(self, nums1: List[int], nums2: List[int]) -> List[int]:
        arr = []
        
        for i in range(len(nums1)):
            count = 0
            for j in range(len(nums2)):
                if nums1[i] == nums2[j]:
                    count += 1
            if count >= 1:
                arr.append(nums1[i])
                
        return arr        